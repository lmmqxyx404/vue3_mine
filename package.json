{
  "private": true,
  "name": "vue3_mine",
  "packageManager": "pnpm@8.6.2",
  "type": "module",
  "version": "3.0.0",
  "description": "imitation of Vue3-core",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "format": "prettier --write --cache --parser typescript \"**/*.[tj]s?(x)\"",
    "format-check": "prettier --check --cache --parser typescript \"**/*.[tj]s?(x)\""
  },
  "keywords": [],
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.17.4",
    "minimist": "^1.2.0",
    "prettier": "^2.7.1"
  }
}
